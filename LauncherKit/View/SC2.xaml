<Page x:Class="LauncherKit.View.SC2"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:LauncherKit.View"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="SC2">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="80"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Image Grid.Row ="0" HorizontalAlignment="Center" VerticalAlignment="Top"
               Stretch="Fill" Source="res/TOPbackground/TopSC2.png"/>
        <Image Grid.Row ="1" HorizontalAlignment="Center" VerticalAlignment="Top"
               Stretch="Fill" Source="res/background/MainSC2.png"/>
    </Grid>
</Page>
